[Nemo Action]
# Use GPrename (apt install gprename) to bulk rename contents of target directory
Name=_Bulk Rename
Comment=Open GPrename in %F
Exec=gprename %F
Icon-Name=document-edit-symbolic
#Stock-Id=
Selection=s
Extensions=dir;
#Mimetypes=
#Separator= 
#Quote=
Dependencies=gprename;
#Conditions=
EscapeSpaces=true
